function Factorial()
	if($1 == 1)
		return ($1);
	endif
	return Factorial($1-1) * $1;
endfunction

function Fibonacci()
	if($1 < 2) 
		return $1;
	endif
	return Fibonacci($1-1) + Fibonacci($1-2);
endfunction	

function Main()
	var ans, value:=10;

	ans := Fibonacci(value);
	Output("Fibo ", value, " := ", ans);

	ans := Factorial(10);
	Output("Fact ", value, " := ", ans);
endfunction
