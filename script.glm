function Factorial()

var fact:=1, n:=1;

if($0 == 0)
	output("Err: no arguments passed to function Factorial");
	return (0);
endif
n:=1;
while( n <= $1)
	Output(fact);
	fact := fact * n;
	n := n + 1;
endwhile
return (fact);
endfunction

function Main()
	var n;
	n := Factorial(10);
	Output("fact 10 := ", n);
endfunction
