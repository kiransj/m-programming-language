function fact()
#sample program for factorial

#variable declartion where variables are initialzed.
var fact:=10, n:=1;

#variable Declartion where they are not initialzied

var sum;
#initialize sum with value 1
sum:=1;

#while loop implemention 
while(n <= fact)
	sum:=sum*n;

	#this is native function it is defined in native_functions.c
	output("fact(",n,") = ", sum);
	n:=n+1;
endwhile

Output(fact, "! is " , sum);

endfunction

function Main()
	fact(10);
endfunction
